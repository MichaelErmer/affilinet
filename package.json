{
  "name": "affilinet-api",
  "version": "0.0.3",
  "description": "Affilinet API Helper Methods For Node / JavaScript",
  "author": {
    "name": "Den Odell",
    "email": "denodell@me.com"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:denodell/affilinet.git"
  },
  "bugs": {
    "url": "https://github.com/denodell/affilinet/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-0 -d dist/ src/",
    "prepublish": "npm run compile",
    "start": "node dist/index.js",
    "test": "npm run compile && ava"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-0"
      ]
    },
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "soap-as-promised": "^1.16.0",
    "source-map-support": "^0.4.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "ava-spec": "^1.0.1",
    "sinon": "^1.17.4",
    "babel-eslint": "^6.1.2",
    "eslint": "^3.1.1"
  }
}
